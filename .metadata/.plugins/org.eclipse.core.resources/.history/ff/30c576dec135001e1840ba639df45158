package com.worldhourproject.bean;

import java.time.LocalDateTime;

public class CountryBean {
	private int id;
	private String country_name;
	private String country_capital;
	private String country_continent;
	//private String country_date_time;
	private LocalDateTime country_date_time;
	
	public CountryBean() {
		
	}
	
	public CountryBean(int id, String country_name, String country_capital, String country_continent, LocalDateTime country_date_time) {
		this.id = id;
		this.country_name = country_name;
		this.country_capital = country_capital;
		this.country_continent = country_continent;
		this.country_date_time = country_date_time;
	}
	
	public CountryBean(String country_name, String country_capital, String country_continent, LocalDateTime country_date_time) {
		this.country_name = country_name;
		this.country_capital = country_capital;
		this.country_continent = country_continent;
		this.country_date_time = country_date_time;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getCountry_name() {
		return country_name;
	}

	public void setCountry_name(String country_name) {
		this.country_name = country_name;
	}

	public String getCountry_capital() {
		return country_capital;
	}

	public void setCountry_capital(String country_capital) {
		this.country_capital = country_capital;
	}

	public String getCountry_continent() {
		return country_continent;
	}

	public void setCountry_continent(String country_continent) {
		this.country_continent = country_continent;
	}

	public LocalDateTime getCountry_date_time() {
		return country_date_time;
	}

	public void setCountry_date_time(LocalDateTime country_date_time) {
		this.country_date_time = country_date_time;
	}	
}
